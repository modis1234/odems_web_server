/* 파일 버튼 */
.file_input_textbox {
    float:left; 
    height:29px;
}
.file_input_div {
    position:relative; 
    width:80px; 
    height:36px; 
    overflow:hidden;
}
.file_input_img_btn {
    padding:0 0 0 5px;
}
.file_input_hidden {
    font-size:29px; 
    position:absolute; 
    right:0px; 
    top:0px; 
    opacity:0; 
    filter: alpha(opacity=0); 
    -ms-filter: alpha(opacity=0); 
    cursor:pointer;
}

input#fileName {
    height: 22px;
    border: 1px solid #cacaca;
    border-radius: 3px;
    color: #000;
    background-color: #fff;
    line-height: normal;
}
form#form {
    height: 24px;
}
img.file_input_img_btn {
    margin-left: 4px;
    position: absolute;
    bottom: 11px;
}

